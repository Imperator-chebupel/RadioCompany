<Window x:Class="База_данных_фирмы.InsertWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:База_данных_фирмы"
        mc:Ignorable="d"
        Title="InsertWindow" Height="245" Width="457" Icon="Images/Second.png">
    <Grid>
        <Button x:Name="InsertButton" HorizontalAlignment="Center" Margin="0,144,0,0" VerticalAlignment="Top" Width="63" Height="64" Click="InsertButton_Click" BorderBrush="Transparent" Background="Transparent">
            <Image Source="Images/Add.png"/>
        </Button>
        <TextBox x:Name="InsertTextBox" HorizontalAlignment="Center" Margin="0,97,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="395"/>
        <TextBox x:Name="SillyTextBox" HorizontalAlignment="Center" Margin="0,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="395" IsReadOnly="True"/>
        <Label x:Name="FirstLabel" Content="Названия параметров" HorizontalAlignment="Left" Margin="31,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="SecondLabel" Content="Значения" HorizontalAlignment="Left" Margin="31,66,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
